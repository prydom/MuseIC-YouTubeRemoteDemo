{
  "manifest_version": 2,
  "name": "SkipYoutube",
  "version": "0.0.1",
  "description": "Skip to next video on YouTube",
  "content_scripts": [
    {
      "matches": ["*://youtube.com/*?enablejsapi=1"],
      "js": ["cs.js"],
      "run_at":"document_end"
    }
  ],
  "web_accessible_resources": [
    "scripts/*.js"
  ]
}

